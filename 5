import java.util.*;
public class tarefafrancis {

    static Scanner s = new Scanner(System.in );
    public static void nmrs(){
        int nm, nm1;
        System.out.println("Digite dois numeros");
        nm = s.nextDouble();
        nm1 = s.nextDouble();
        if(nm > nm1)
        {
            System.out.println(nm + "é o maior numero");

        }
        else
        {
            System.out.println(nm1 + "é o maior numero");
        }
    
    }
    public static void main(String[] args) {
        nmrs();
    }


}
